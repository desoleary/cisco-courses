<% if(!this.contactdata) return; %>

<table class="contactdata" id="contactgrid">
	<tr>
		<th>Contact Id</th><th class="name">Name</th><th class="addr">Address</th><th>Phone Num</th><th>Phone Type</th><th>Email</th><th class="company">Company</th><th>Job Title</th>
	</tr>
    <% $.each(this.contactdata, function(all, contact) {  %>
         <tr>
         	 <td><%= contact.contactid %></td>
             <td><%= contact.name %></td>
             <td><%= contact.address %></td>
             <td><%= contact.phones[0].number %></td>
             <td><%= contact.phones[0].type %></td>
             <td><%= contact.email %></td>
             <td><%= contact.company %></td>
             <td><%= contact.position %></td>
         </tr>
    <% }) %>
</table>